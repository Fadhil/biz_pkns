%header.navbar
  .row
    .three.columns.logo
      - path = current_user ? '/home' : root_url
      = link_to path do
        = image_tag 'logoPKNSBIZ.png', height: '68'
    %h4.six.columns
      .three.columns.small
        = session[:current_program_name] unless session[:current_program_name].nil?
    %ul.three.columns
      = render 'layouts/navigation'