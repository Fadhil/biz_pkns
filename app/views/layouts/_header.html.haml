%header.navbar
  .row
    %h5.nine.columns.logo
      .six.columns
        = image_tag 'pknslogo.png'
        - if current_user
          = link_to 'PKNS BIZ DATA', '/home'
        - else
          = link_to 'PKNS BIZ DATA', root_url
      .five.columns.small
        = session[:current_program_name] unless session[:current_program_name].nil?#, '#'
    / %ul.three.columns.small
    /   = session[:current_program_name] unless session[:current_program_name].nil?#, '#'
    %ul.three.columns
      = render 'layouts/navigation'