.row
  .twelve.columns
    %h4=t('.title')
  %hr.divider
.row
  %ul.four_up.tiles
    - (0..5).each do |index|
      %li.panel.center
        .image
          = image_tag(@members[index].profile_photo.try(:file), size: "100x100")
        .name
          %h5
            = link_to @members[index].full_name, '#'
        .status
          / %small.label.info Status semasa :
          %section.label.info
            = t("reports.users.employment.#{@members[index].current_employment_status}")
        .program
          %ul
            - @members[index].programs.uniq.each do |program|
              %li
                %small= program.name