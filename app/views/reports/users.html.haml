.row
  %h3.twelve.columns
    =t '.title'
.row
  .twelve.columns
    %hr
    %section.vertical.tabs
      %ul.tab-nav.four.columns
        %li.active
          = link_to t('.user_details'), '#', onClick: 'hideChart("usersprogramchart");'
        / %li
        /   = link_to t('.business_details'), '#'
        %li
          = link_to t('member_nonmember_by_age'), '#', onClick: 'hideChart("usersprogramchart");'
        %li
          = link_to t('member_gender_by_age'), '#', onClick: 'hideChart("usersprogramchart");'
        %li
          = link_to t('member_by_current_status'), '#', onClick: 'hideChart("usersprogramchart");'
        %li
          = link_to t('member_by_program'), '#',onClick: 'showChart("usersprogramchart");'
        %li
          = link_to t('member_by_business_details'), '#', onClick: 'hideChart("usersprogramchart");'
      .tab-content.eight.columns.active
        = render partial: 'users_members_non_members'
      / .tab-content.eight.columns
      /   = render partial: 'users_with_business'
      .tab-content.eight.columns
        = render partial: 'users_by_age'
      .tab-content.eight.columns
        = render partial: 'users_by_gender'
      .tab-content.eight.columns
        = render partial: 'users_by_current_status'
      .tab-content.eight.columns
        = render partial: 'users_with_business' 
      .tab-content.eight.columns
        = render partial: 'users_by_business'
    
.row
  .twelve.columns
    #usersprogramchart.hide{style: "width: 900px; height: 800px"}
